[searchcomments]
[fullresult]

<div id="dle-ajax-comments"></div>
    <div class="bcomment">
  <div class="bccomm">
    <div class="lcol avacom"><img src="{foto}" alt=""/></div>
    <div class="rcol">
      [fast]<b class="cnum">#{comment-id} [/fast]</b>
      <span class="cdate">{result-date}</span>
      <div class="bctop">
       {result-author}
      </div>
      <div class="bccont">
          {result-text}
          
        <div class="clr"></div>
      </div>
      
      
      <ul class="comedit">
        [not-group=5]

